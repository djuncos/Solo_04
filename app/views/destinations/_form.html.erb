<%= form_for(@destination) do |f| %>

	<div class="field new_destination">
	   <%= f.label :address, "Enter destination address"%>
	   <div class="new_destination_address">
	   		<%= f.text_field :address %>
	   </div>

	   <%= f.label :user_act_latitude, "User's current latitude"%>
	   <div class="new_destination_user_lat">
	   		<%= f.text_field :user_act_latitude %>
	   </div>

	   <%= f.label :user_act_longitude, "User's current longitude"%>
	   <div class="new_destination_user_long">
	   		<%= f.text_field :user_act_longitude %>
	   </div>



	   <div class="actions">
	    <%= f.submit class: "btn btn-primary"%>
	  </div>
	</div>




<% end %>